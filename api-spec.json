        "tags": [
          "stock-controller"
        ],
        "summary": "Make and order for amount of milk and wool",
        "operationId": "orderUsingPOST",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "order",
            "description": "order",
            "required": true,
            "schema": {
              "$ref": "#/definitions/OrderDTO"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/OrderDTO"
            }
          },
          "201": {
            "description": "Created"
          },
          "202": {
            "description": "Order was successfully processed",
            "schema": {
              "$ref": "#/definitions/OrderDTO"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "422": {
            "description": "Could not process order - insufficient amount of resources"
          }
        }
      }
    },
